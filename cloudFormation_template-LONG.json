      "Value": { "Ref": "VPC" }
    },
    "PublicSubnetId": {
      "Description": "ID of the public subnet",
      "Value": { "Ref": "PublicSubnet" }
    },
    "InstanceSecurityGroupId": {
      "Description": "ID of the instance security group",
      "Value": { "Ref": "InstanceSecurityGroup" }
    },
    "InstanceId1": {
      "Description": "Instance ID of WebAppInstance1",
      "Value": { "Ref": "WebAppInstance1" }
    },
    "InstancePublicIp1": {
      "Description": "Public IP of WebAppInstance1",
      "Value": { "Fn::GetAtt": ["WebAppInstance1", "PublicIp"] }
    },
    "DataBucketName1": {
      "Description": "Name of the S3 data bucket 1",
      "Value": { "Ref": "DataBucket1" }
    },
    "ReadOnlyRoleArn1": {
