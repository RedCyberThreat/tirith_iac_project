{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "A very large, 5000+ line example CloudFormation template. This template creates a VPC, a security group, and a large number of EC2 instances, S3 buckets, IAM roles, and CloudWatch alarms for demonstration purposes.",
  "Parameters": {
    "VpcCIDR": {
      "Description": "CIDR block for the VPC",
      "Type": "String",
      "Default": "10.0.0.0/16"
    },
    "SubnetCIDR": {
      "Description": "CIDR block for the public subnet",
      "Type": "String",
      "Default": "10.0.1.0/24"
    },
    "InstanceType": {
      "Description": "EC2 instance type",
      "Type": "String",
      "Default": "t2.micro",
      "AllowedValues": ["t2.micro", "t2.small", "t3.micro", "t3.small"]
    },
    "KeyName": {
      "Description": "Name of an existing EC2 KeyPair to enable SSH access to the instances",
      "Type": "AWS::EC2::KeyPair::KeyName",
      "ConstraintDescription": "must be the name of an existing EC2 KeyPair."
    },
    "ProjectName": {
      "Description": "The name of the project",
      "Type": "String",
      "Default": "LargeScaleDemo"
    },
    "Environment": {
      "Description": "The deployment environment",
      "Type": "String",
      "Default": "Development",
      "AllowedValues": ["Development", "Staging", "Production"]
    }
  },
  "Mappings": {
    "RegionMap": {
      "us-east-1": {
        "AMI": "ami-0c55b159cbfafe1f0"
      },
      "us-east-2": {
        "AMI": "ami-0c55b159cbfafe1f0"
      },
      "us-west-1": {
        "AMI": "ami-0c55b159cbfafe1f0"
      },
      "us-west-2": {
        "AMI": "ami-0c55b159cbfafe1f0"
      },
      "eu-west-1": {
        "AMI": "ami-0c55b159cbfafe1f0"
      },
      "eu-central-1": {
        "AMI": "ami-0c55b159cbfafe1f0"
      }
    }
  },
  "Resources": {
    "VPC": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": {
          "Ref": "VpcCIDR"
        },
        "EnableDnsSupport": true,
        "EnableDnsHostnames": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": { "Fn::Sub": "${ProjectName}-VPC" }
          }
        ]
      }
    },
    "InternetGateway": {
      "Type": "AWS::EC2::InternetGateway",
      "Properties": {
        "Tags": [
          {
            "Key": "Name",
            "Value": { "Fn::Sub": "${ProjectName}-IGW" }
          }
        ]
      }
    },
    "VPCGatewayAttachment": {
      "Type": "AWS::EC2::VPCGatewayAttachment",
      "Properties": {
        "VpcId": {
          "Ref": "VPC"
        },
        "InternetGatewayId": {
          "Ref": "InternetGateway"
        }
      }
    },
    "PublicSubnet": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "VPC"
        },
        "CidrBlock": {
          "Ref": "SubnetCIDR"
        },
        "MapPublicIpOnLaunch": true,
        "Tags": [
          {
            "Key": "Name",
            "Value": { "Fn::Sub": "${ProjectName}-PublicSubnet" }
          }
        ]
      }
    },
    "RouteTable": {
      "Type": "AWS::EC2::RouteTable",
      "Properties": {
        "VpcId": {
          "Ref": "VPC"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": { "Fn::Sub": "${ProjectName}-RouteTable" }
          }
        ]
      }
    },
    "PublicRoute": {
      "Type": "AWS::EC2::Route",
      "DependsOn": "VPCGatewayAttachment",
      "Properties": {
        "RouteTableId": {
          "Ref": "RouteTable"
        },
        "DestinationCidrBlock": "0.0.0.0/0",
        "GatewayId": {
          "Ref": "InternetGateway"
        }
      }
    },
    "SubnetRouteTableAssociation": {
      "Type": "AWS::EC2::SubnetRouteTableAssociation",
      "Properties": {
        "SubnetId": {
          "Ref": "PublicSubnet"
        },
        "RouteTableId": {
          "Ref": "RouteTable"
        }
      }
    },
    "InstanceSecurityGroup": {
      "Type": "AWS::EC2::SecurityGroup",
      "Properties": {
        "VpcId": {
          "Ref": "VPC"
        },
        "GroupDescription": "Enable HTTP and SSH access",
        "SecurityGroupIngress": [
          {
            "IpProtocol": "tcp",
            "FromPort": 80,
            "ToPort": 80,
            "CidrIp": "0.0.0.0/0"
          },
          {
            "IpProtocol": "tcp",
            "FromPort": 22,
            "ToPort": 22,
            "CidrIp": "0.0.0.0/0"
          }
        ],
        "Tags": [
          {
            "Key": "Name",
            "Value": { "Fn::Sub": "${ProjectName}-InstanceSG" }
          }
        ]
      }
    },
    "WebAppInstance1": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "InstanceType": { "Ref": "InstanceType" },
        "KeyName": { "Ref": "KeyName" },
        "ImageId": {
          "Fn::FindInMap": ["RegionMap", { "Ref": "AWS::Region" }, "AMI"]
        },
        "SubnetId": { "Ref": "PublicSubnet" },
        "SecurityGroupIds": [{ "Ref": "InstanceSecurityGroup" }],
        "UserData": {
          "Fn::Base64": "#!/bin/bash\nyum update -y\nyum install -y httpd\nsystemctl start httpd\nsystemctl enable httpd\necho '<h1>Hello from Instance 1</h1>' > /var/www/html/index.html"
        },
        "Tags": [
          { "Key": "Name", "Value": { "Fn::Sub": "${ProjectName}-WebApp-1" } },
          { "Key": "Environment", "Value": { "Ref": "Environment" } }
        ]
      }
    },
    "CPUAlarmForInstance1": {
      "Type": "AWS::CloudWatch::Alarm",
      "Properties": {
        "AlarmName": { "Fn::Sub": "${ProjectName}-CPU-High-Instance-1" },
        "AlarmDescription": "Alarm when CPU exceeds 70% for instance 1",
        "Namespace": "AWS/EC2",
        "MetricName": "CPUUtilization",
        "Dimensions": [
          { "Name": "InstanceId", "Value": { "Ref": "WebAppInstance1" } }
        ],
        "Statistic": "Average",
        "Period": 300,
        "EvaluationPeriods": 1,
        "Threshold": 70,
        "ComparisonOperator": "GreaterThanThreshold"
      }
    },
    "DataBucket1": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": {
          "Fn::Sub": "${aws::stackname}-data-bucket-1-${aws::accountid}"
        },
        "LifecycleConfiguration": {
          "Rules": [
            {
              "Id": "TransitionToIARule",
              "Status": "Enabled",
              "Transitions": [
                { "StorageClass": "STANDARD_IA", "TransitionInDays": 30 }
              ]
            },
            {
              "Id": "TransitionToGlacierRule",
              "Status": "Enabled",
              "Transitions": [
                { "StorageClass": "GLACIER", "TransitionInDays": 90 }
              ]
            },
            {
              "Id": "ExpirationRule",
              "Status": "Enabled",
              "ExpirationInDays": 365
            }
          ]
        }
      }
    },
    "ReadOnlyRole1": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": { "Service": "ec2.amazonaws.com" },
              "Action": "sts:AssumeRole"
            }
          ]
        },
        "Path": "/",
        "Policies": [
          {
            "PolicyName": "S3ReadOnlyAccess",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": ["s3:Get*", "s3:List*"],
                  "Resource": "*"
                }
              ]
            }
          }
        ]
      }
    },
    "WebAppInstance150": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "InstanceType": { "Ref": "InstanceType" },
        "KeyName": { "Ref": "KeyName" },
        "ImageId": {
          "Fn::FindInMap": ["RegionMap", { "Ref": "AWS::Region" }, "AMI"]
        },
        "SubnetId": { "Ref": "PublicSubnet" },
        "SecurityGroupIds": [{ "Ref": "InstanceSecurityGroup" }],
        "UserData": {
          "Fn::Base64": "#!/bin/bash\nyum update -y\nyum install -y httpd\nsystemctl start httpd\nsystemctl enable httpd\necho '<h1>Hello from Instance 150</h1>' > /var/www/html/index.html"
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": { "Fn::Sub": "${ProjectName}-WebApp-150" }
          },
          { "Key": "Environment", "Value": { "Ref": "Environment" } }
        ]
      }
    },
    "CPUAlarmForInstance150": {
      "Type": "AWS::CloudWatch::Alarm",
      "Properties": {
        "AlarmName": { "Fn::Sub": "${ProjectName}-CPU-High-Instance-150" },
        "AlarmDescription": "Alarm when CPU exceeds 70% for instance 150",
        "Namespace": "AWS/EC2",
        "MetricName": "CPUUtilization",
        "Dimensions": [
          { "Name": "InstanceId", "Value": { "Ref": "WebAppInstance150" } }
        ],
        "Statistic": "Average",
        "Period": 300,
        "EvaluationPeriods": 1,
        "Threshold": 70,
        "ComparisonOperator": "GreaterThanThreshold"
      }
    },
    "DataBucket150": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": {
          "Fn::Sub": "${aws::stackname}-data-bucket-150-${aws::accountid}"
        },
        "LifecycleConfiguration": {
          "Rules": [
            {
              "Id": "TransitionToIARule",
              "Status": "Enabled",
              "Transitions": [
                { "StorageClass": "STANDARD_IA", "TransitionInDays": 30 }
              ]
            },
            {
              "Id": "TransitionToGlacierRule",
              "Status": "Enabled",
              "Transitions": [
                { "StorageClass": "GLACIER", "TransitionInDays": 90 }
              ]
            },
            {
              "Id": "ExpirationRule",
              "Status": "Enabled",
              "ExpirationInDays": 365
            }
          ]
        }
      }
    },
    "ReadOnlyRole150": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": { "Service": "ec2.amazonaws.com" },
              "Action": "sts:AssumeRole"
            }
          ]
        },
        "Path": "/",
        "Policies": [
          {
            "PolicyName": "S3ReadOnlyAccess",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": ["s3:Get*", "s3:List*"],
                  "Resource": "*"
                }
              ]
            }
          }
        ]
      }
    }
  },
